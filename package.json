    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^15.0.3",
    "mp4box": "1.4.6",
    "next": "^15",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
